{
  "manifest_version": 2,
  "name": "Facebook Slient Mode",
  "short_name": "Facebook Slient Mode",
  "description": "Facebook Slient Mode",
  "version": "1.0.0",
  "author": "matico",
  "permissions": [
    "webRequest", 
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
        "js/background.js"
    ],
    "persistent": true
  },
  "web_accessible_resources": [
    "data/*.json"
  ],
  "browser_action":{
    "default_icon": "icon/enable.png"
  }
}